   "source": [
    "N = int(input())\n",
    "M = int(input())\n",
    "S = input()\n",
    "start = end = 0\n",
    "result = 0\n",
    "while end + 3 <= M:\n",
    "    if S[end:end+3] == \"IOI\":\n",
    "        end += 2\n",
    "        if end-start == 2 * N:\n",
    "            result += 1\n",
    "            start += 2\n",
    "    else:\n",
    "        start = end = end + 1\n",
    "print(result)"
   ]
